#md5sums=("SKIP")

prepare() {
	cp -r ../${pkgname}/* ${srcdir}
}

build() {
	echo
}

check() {
	echo
}

package() {
	sudo install -Dm 644 ${srcdir}/vconsole.conf -t ${pkgdir}/etc
}
